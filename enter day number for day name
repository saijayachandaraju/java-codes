
public class statement {
	public static void main(String[] args) {
		int day=5;
		String daystring;
		switch(day){
		case 1:
			daystring ="monday";
			break;
		case 2:
			daystring="tuesday";
			break;
		case 3:
			daystring="wednesday";
			break;
		case 4:
			daystring="thursday";
			break;
		case 5:
			daystring="friday";
			break;
		default:
			daystring="invalid day";
			
			
		
		}
		System.out.println(daystring);
	}

}
