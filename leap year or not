
public class statement2 {
	public static void main(String[] args) {
		int year=2000;
		switch(year){
		case 0:
			break;
		default:
			if(year%4==0 & year%100==0 & year%400==0) {
				System.out.println("leapyear");
			}
			else {
				System.out.println("not leap year");
				
			}
			
		
		}
		
	}

}
